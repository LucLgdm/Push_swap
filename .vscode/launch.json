{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug push_swap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/push_swap",
            "args": ["4 25 12 0 -56 21"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
